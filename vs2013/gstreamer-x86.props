<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<ImportGroup Label="PropertySheets" />
	<PropertyGroup Label="UserMacros">
		<GSTREAMER_SDK_DIR>D:\sdk\gstreamer\1.0\x86\</GSTREAMER_SDK_DIR>
	</PropertyGroup>
	<PropertyGroup>
		<IncludePath>$(GSTREAMER_SDK_DIR)\include;$(IncludePath)</IncludePath>
		<IncludePath>$(GSTREAMER_SDK_DIR)\include\glib-2.0;$(IncludePath)</IncludePath>
		<IncludePath>$(GSTREAMER_SDK_DIR)\include\gstreamer-1.0;$(IncludePath)</IncludePath>
		<IncludePath>$(GSTREAMER_SDK_DIR)\lib\glib-2.0\include;$(IncludePath)</IncludePath>
		<LibraryPath>$(GSTREAMER_SDK_DIR)\lib\;$(LibraryPath)</LibraryPath>
		<ExecutablePath>$(GSTREAMER_SDK_DIR)\bin\;$(ExecutablePath)</ExecutablePath>
	</PropertyGroup>
	<ItemDefinitionGroup>
		<Link>
			<AdditionalDependencies>gobject-2.0.lib;glib-2.0.lib;gstreamer-1.0.lib;gstapp-1.0.lib;%(AdditionalDependencies)</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
</Project>
